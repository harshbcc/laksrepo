pipeline {
    agent any
    stages {
        stage('build') {
            steps {
                echo 'Building the file..'
            }
            }
            stage('Test') {
            steps {
                echo 'Testing the file..'
            }
        }
         stage('Deploy') {
            steps {
                echo 'Deploying the file..'
            }
        }
    }
}
